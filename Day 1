#include<iostream>
using namespace std;

int main()
{
    int t;
    cin>>t;
    
    while(t--)
    {
        int x,y;
        cin>>x>>y;
        
        if(x<=y)
        {
            cout<<0;
        }
        else
        {
            cout<<x-y;
        }
        cout<<endl;
    }
    return 0;
}
