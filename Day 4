#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        string s;
        cin>>s;
        bool flag=false;
        for(auto i:s)
        {    if(i == '0' || i == '5')
                flag=true;
        }
        if(flag == true)
            std::cout << "YES" << std::endl;
        else
        std::cout << "NO" << std::endl;
    }
}
