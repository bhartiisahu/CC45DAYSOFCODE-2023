#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,m;
	cin>>t;
	while (t--)
	{
	    cin>>n>>m;
	    int temp=n/m;
	    if(n%m==0 && temp%2==0)
	    {
	        cout<<"yes"<<endl;
	        
	       
	    }
	    else
	    {
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
