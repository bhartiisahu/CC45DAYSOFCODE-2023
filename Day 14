#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int T;
    cin >> T;
    
    while (T--) {
        int X, Y, Z;
        cin >> X >> Y >> Z;
        
       
        if ((X + Y == Z) || (Y + Z == X) || (Z + X == Y)) {
            cout << "yes" << endl;
        } else {
            cout << "no" << endl;
        }
    }
    
    return 0;
}
