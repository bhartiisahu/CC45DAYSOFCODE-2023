#include <iostream>
using namespace std;

int main() {
	int t,a1,a2,a3,a4,a5,P;
	cin>>t;
	while (t--)
	{
	    cin>>a1>>a2>>a3>>a4>>a5>>P;
	    int sum=a1+a2+a3+a4+a5;
	    int hr=(P*5)/sum;
	    if(hr<=P)
	    {
	        cout<<"No"<<endl;
	    }
	    else
	    {
	        cout<<"Yes";
	    }
	    
	}
	return 0;
}
