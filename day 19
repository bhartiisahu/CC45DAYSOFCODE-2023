#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,a,b,c,d;
	cin>>t;
	while (t--)
	{
	    cin>>a>>b>>c>>d;
	    if(max(a,b)<max(c,d))
	    {
	        cout<<"P"<<endl;
	    }
	    else if(max(a,b)>max(c,d))
	    {
	        cout<<"Q"<<endl;
	    }
	    else{
	        cout<<"TIE"<<endl;
	    }
	    
	    
	}
	return 0;
}
